<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
		<java.version>1.8</java.version>
		<maven.compiler.source>1.8</maven.compiler.source>
		<maven.compiler.target>1.8</maven.compiler.target>
		<spring.boot.version>2.1.2.RELEASE</spring.boot.version>
		<apache.httpclient.version>4.5.7</apache.httpclient.version>
		<apache.commons-lang3.version>3.8.1</apache.commons-lang3.version>
		<nexus.id>JinTechEng</nexus.id>
		<nexus.name>JinTechEngMergeNexus</nexus.name>
<!-- 		<nexus.repository>http://ojooy.iptime.org:8081/service/rest/repository/browse/JinTechEng/</nexus.repository> -->
<!-- 		<nexus.snapshot.repository>http://ojooy.iptime.org:8081/service/rest/repository/browse/maven-snapshots/</nexus.snapshot.repository> -->
		<nexus.repository>http://ojooy.iptime.org:8081/repository/JinTechEng/</nexus.repository>
		<nexus.snapshot.repository>http://ojooy.iptime.org:8081/repository/maven-snapshots/</nexus.snapshot.repository>
		<nexus.upload.repository>http://ojooy.iptime.org:8081/repository/JinTechEng/</nexus.upload.repository>
	</properties>
	
	
	<groupId>kr.co.itcall.jte</groupId>
	<artifactId>JinTechEngParents</artifactId>
	<version>1.0.0</version>
	<packaging>pom</packaging>
	<name>JinTechEngParents</name>
	<description>Maven Parents Project for JinTechEng</description>
	
	
	<parent>
		<groupId>org.springframework.boot</groupId>
		<artifactId>spring-boot-starter-parent</artifactId>
		<version>2.1.2.RELEASE</version>
		<relativePath/> <!-- lookup parent from repository -->
	</parent>
	
	
	<repositories>
		<repository>
			<id>${nexus.id}</id>
			<name>${nexus.name}</name>
			<releases>
				<enabled>true</enabled>
			</releases>
			<url>${nexus.repository}</url>
		</repository>
<!-- 		<repository> -->
<!-- 			<id>local</id> -->
<!-- 			<name>Local Repository</name> -->
<!-- 			<url>file://${project.basedir}/../../LocalRepository/</url> -->
<!-- 				or -->
<!-- 			<url>file://D:/JTE/LocalRepository/</url> -->
<!-- 		</repository> -->
<!-- 		<repository> -->
<!-- 			<id>mesir-repo</id> -->
<!-- 			<url>https://simulation.tudelft.nl/maven/</url> -->
<!-- 		</repository> -->
		<repository>
			<id>maven1</id>
			<name>maven1 Repository</name>
			<url>https://repo1.maven.org/maven2/</url>
		</repository>
		<repository>
			<id>maven2</id>
			<name>maven2 Repository</name>
			<url>http://repo2.maven.org/maven2/</url>
		</repository>
		<repository>
			<id>apache</id>
			<name>maven2 apache Repository</name>
			<url>http://repo.maven.apache.org/maven2/</url>
		</repository>
		<repository>
			<id>java-net-repo</id>
			<url>https://maven.java.net/content/repositories/public/</url>
		</repository>
		<repository>
			<id>jboss-repo</id>
			<url>http://repository.jboss.org/nexus/content/groups/public/</url>
		</repository>
		<repository>
			<id>spring-repo</id>
			<url>https://repo.spring.io/release</url>
		</repository>
	</repositories>
	<pluginRepositories>
		<pluginRepository>
			<id>${nexus.id}</id>
			<name>${nexus.id} Plugin Repository</name>
			<url>${nexus.repository}</url>
			<layout>default</layout>
			<snapshots>
				<enabled>false</enabled>
			</snapshots>
			<releases>
				<updatePolicy>never</updatePolicy>
			</releases>
		</pluginRepository>
		<pluginRepository>
			<id>central</id>
			<name>Maven Plugin Repository</name>
			<url>https://repo1.maven.org/maven2/</url>
			<layout>default</layout>
			<snapshots>
				<enabled>false</enabled>
			</snapshots>
			<releases>
				<updatePolicy>never</updatePolicy>
			</releases>
		</pluginRepository>
	</pluginRepositories>
	<distributionManagement>
		<repository>
			<id>${nexus.id}</id>
			<name>${nexus.name}</name>
			<url>${nexus.upload.repository}</url>
		</repository>
		<snapshotRepository>
			<id>maven-snapshots</id>
			<name>JinTechEng Snapshot Repository</name>
			<url>${nexus.snapshot.repository}</url>
		</snapshotRepository>
	</distributionManagement>
	
	
	<dependencies>
		<dependency>
			<groupId>org.apache.httpcomponents</groupId>
			<artifactId>httpclient</artifactId>
			<!-- <version>${apache.httpclient.version}</version> -->
		</dependency>

		<dependency>
			<groupId>org.apache.commons</groupId>
			<artifactId>commons-lang3</artifactId>
			<!-- <version>${apache.commons-lang3.version}</version> -->
		</dependency>

		<!-- MultiPartFileUpload관련 Upload라이브러리. -->
		<dependency>
			<groupId>commons-fileupload</groupId>
			<artifactId>commons-fileupload</artifactId>
			<version>1.4</version>
		</dependency>

		<dependency>
			<groupId>org.projectlombok</groupId>
			<artifactId>lombok</artifactId>
			<optional>true</optional>
		</dependency>

		<!-- LOG -->
		<dependency>
			<groupId>org.lazyluke</groupId>
			<artifactId>log4jdbc-remix</artifactId>
			<version>0.2.7</version>
		</dependency>

		<!-- 개발시 자동적용하기 -->
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-devtools</artifactId>
			<optional>true</optional>
		</dependency>
		
		<dependency>
			<groupId>org.apache.poi</groupId>
			<artifactId>poi</artifactId>
			<version>3.11</version>
		</dependency>
		<dependency>
			<groupId>org.apache.poi</groupId>
			<artifactId>poi-ooxml</artifactId> <!-- 엑셀 2007 이상 버전에서 사용 -->
			<version>3.11</version>
		</dependency>

	</dependencies>



	<modules>
		<module>JteComCore</module>
		<module>WebAppCore</module>
		<module>WebViewHandlebars</module>
		<module>WebViewJSP</module>
		<module>WebViewThymeleaf</module>
		<module>WebViewFreemarker</module>
		<module>MesBasic</module>
		<module>MesStaticBasic</module>
		<module>MesSinMyung</module>
		<module>MesStaticSinMyung</module>
		<module>MesHana</module>
		<module>MesStaticHana</module>
		<module>MesLsk</module>
		<module>MesStaticLsk</module>
		<module>MesZinix</module>
		<module>MesStaticZinix</module>
	</modules>
	
	
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-plugins</artifactId>
				<version>31</version>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-clean-plugin</artifactId>
				<!-- <version>3.1.0</version> -->
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-deploy-plugin</artifactId>
<!-- 				<version>2.8.2</version> -->
				<configuration>
					<skip>true</skip>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.eclipse.m2e</groupId>
				<artifactId>lifecycle-mapping</artifactId>
				<configuration>
					<skip>true</skip>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.sonatype.plugins</groupId>
				<artifactId>nexus-staging-maven-plugin</artifactId>
				<version>1.6.8</version>
				<executions>
					<execution>
						<id>default-deploy</id>
						<phase>deploy</phase>
						<goals>
							<goal>deploy</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<serverId>${nexus.id}</serverId>
					<nexusUrl>${nexus.repository}</nexusUrl>
					<skipStaging>true</skipStaging>
				</configuration>
			</plugin>
		</plugins>
	</build>
	
	
</project>