
@CHARSET "UTF-8";

.grid-content {
	width: 100%;
}

.rlehoMgtForm {
	width: 100%;
	margin: 0 auto;
/* 	text-align: center; */
	vertical-align: middle;
}

.rlehoMgtForm .left-contents {
	width: 20%;
	display: inline-block;
	height: 100%;
}

.rlehoMgtForm .middle-contents {
	width: 5%;
	display: inline-block;
	text-align: center;
	vertical-align: middle;
}

.rlehoMgtForm .right-contents {
	width: 74%;
	display: inline-block;
}

.middle-contents .fas.fa-caret-square-right.fa-3x{
	color: #3367D6;
	padding-right: 24px;
	
}

.rlehoMgtForm #jteMasterTree {
	font-size: 16px;
	border: 2px solid #8aafff;
	background:#ffffff;
}

/** 20.08.10 JJW 왼쪽 트리 구조 여백 따로 주기 위함**/
.rlehoMgtForm.k-treeview .k-item {
	margin-left: 10px;
	margin-top: 10px;
}

#jtePopForm_master {
	margin-top: 5px;
}

.menuInfoForm .col-left-title {
	width: 20%;
}

.menuInfoForm .col-right-content {
	width: 70%;
}

.menuInfoForm .group-checkbox {
	overflow: auto;
	height: 150px;
	background-color: #ffffff;
	padding-top: 5px;
	border: 1px solid #C7D5F4;
}

.menuInfoForm .jtePopForm_master th {
   text-align: left;
}

.rlehoMgtForm .left-input {
   width: 100%;
   float: left;
}

.rlehoMgtForm .right-input {
   width: 100%;
   float: right;
}

.rlehoMgtForm .center-input {
	width: 100%;
}

.rlehoMgtForm .normal-input {
	width: 100%;
}

.menuInfoForm .search-btn {
	width: 90%;
	float: right;
}


/*********************************/


.rlehoMgtForm #jtePopForm_master {
	padding-top: 15px;
}

.compMgtForm .jtePopForm_master {
	width: 100%;
}

.grid-content {
   width: 100%;
}

/*04.09 JJW 내부 스타일 옮김*/
.compMgtForm .k-listbox {
    box-sizing: border-box;
    height: 300px;
    /*** listbox inline 표시 위해_gwpark ***/
    display: inline-flex; 
}

.compMgtForm #jtePopForm_form {
	text-align: left;
	padding-left: 0;
}

.compMgtForm .jtePopForm_master {
	width: 100%;
	margin-left: 10px;
	margin-top: 10px;
}

.compMgtForm .jtePopForm_master td {
	padding-top: .45em;
	padding-bottom: .45em;
}

/* .k-widget .k-listbox {
	width: 40%;
	padding-left: 10px;
}
 */
.k-widget .k-listbox .k-listbox-toolbar-right {
	width: 45%;
}

.k-listbox-toolbar {
	margin-left: 5px;
	margin-right: 5px;
}

.final-btn-area {
	margin-top: 0;
	padding-top: 0;
	text-align: center;
	
}


div .k-widget .k-window {
	height: 440px;
	width: 650px;
}

.rlehoMgtForm .col-left-title {
	width: 17%;
}

.rlehoMgtForm .col-left-content-1 {
	width: 20%;
}

.rlehoMgtForm .col-left-content-2 {
	width: 12%;
}

.rlehoMgtForm .col-right-title {
	width: 19%;
}

.rlehoMgtForm .col-right-content-1 {
	width: 22%;
}

.rlehoMgtForm .col-right-content-2 {
	width: 10%;
}

.rlehoMgtForm .grid-content {
	width: 100%;
}

.rlehoMgtForm .normal-input {
	width: 100%;
}

.rlehoMgtForm .td-right-title {
	width: 90%;
	float: right;
	margin-top: 5px;
}

.rlehoMgtForm .left-input.btn-input{
	width: 95%;
	float: left;
}

.rlehoMgtForm .search-btn{
	width: 100%;
}

