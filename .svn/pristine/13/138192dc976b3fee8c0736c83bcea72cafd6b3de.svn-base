	width:100%;
}

.routingMgtForm .jtePopForm_master th {
	text-align: left;
}

.routingMgtForm .jtePopForm_master td {
	padding-top: .45em;
	padding-bottom: .45em;
} /*팝업 그리드인듯*/

.routingMgtForm .jteRoutingGrid{  /* 200416 그리드 고정 + 스크롤_LYM */
     width: 100%;
     table-layout: fixed;
     vertical-align: top;
}

.routingMgtForm .k-numerictextbox .k-numeric-wrap .k-input {
	padding: 4px 0px;
}

.routingMgtForm .left-content-box {
	vertical-align: top;
	width: 31%;
}

.routingMgtForm .right-content-box {
	border-style: solid; 
	border-image: linear-gradient(180deg, rgba(8,19,50,1) 0%, rgba(79,98,121,1) 50%, rgba(150,166,186,1) 100%);
	border-image-slice:1;
	padding: 0px 26px;
	width:69%;
}

.routingMgtForm .tb-col-left-content{  /* 200416 그리드 고정 + 스크롤_LYM */
     width: 31%;
     overflow: auto;
}

.routingMgtForm .tb-col-right-content{
	width:69%;
}

#popArea .k-grid-toolbar {
	padding: .22em .2em .28em;
}

#popArea td {
	padding: .4em .6em;
}

