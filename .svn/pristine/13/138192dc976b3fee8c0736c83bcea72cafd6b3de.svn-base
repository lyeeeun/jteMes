}


/************설비***********/


.processInfoForm.equipment_info_div{
	text-align: center;
	margin-top: 0.8em;
} 

.processInfoForm .equipment_info_left {
	width:49%;
	float: left;
	text-align: center;
	font-weight: bold;
}

.processInfoForm .equipment_info_right {
	width:49%;
	float: right;
	padding-right: 2%;
	text-align: center;
	font-weight: bold;
}

.processInfoForm .equipmentList {
	width: 45%;
}

.processInfoForm .search-area{
	padding: 0.3em 0em;
	background: linear-gradient(90deg, rgba(47,69,91,1) 0%, rgba(47,69,91,1) 47.8%, rgba(255,255,255,1) 47.9%, rgba(255,255,255,1) 50%, rgba(255,255,255,1) 50%, rgba(47,69,91,1) 50%, rgba(47,69,91,1) 100%);
	margin-left: 1em;
	margin-right: 1em;
	margin-bottom: 0.8em;
}

.processInfoForm .searchEqmt{
	width:41%;
	margin-right: 0.6em;
	margin-left: -0.3em;
}

.processInfoForm .searchSelectedEqmt{
	width: 43%;
	margin-left: 4.4%;
	margin-right: 0.5em;
}

.processInfoForm .jtePopForm_master {
	width: 100%;
	padding-left: 3.5%;
	padding-right: 3.5%;
}


.processInfoForm .selectedEquipment {
	width: 40%;
}


.processInfoForm .grid-content {
	width: 100%;
}

.processInfoForm .k-listbox {
    box-sizing: border-box;
    height: 300px;
    /** listbox inline 표시 위해_gwpark **/
    display: inline-flex; 
    margin-top: 2%;
}

.processInfoForm .k-numerictextbox .k-numeric-wrap .k-input {
	padding: 4px 0px;
}

.processInfoForm .k-listbox.k-listbox-toolbar-right .k-listbox-toolbar {
    margin-left: 8px;
    margin-right: 4px; /* 200409 툴바 양옆으로 마진 부여_LYM */
}



/************공구***********/

.routingMgtForm{
	margin-top: 0.8em;	
}

.toolInfoPopup_form{
	width: 93%;
