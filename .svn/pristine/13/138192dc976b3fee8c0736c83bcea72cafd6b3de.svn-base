	width: 100%;
	margin-top: 0.8em;
	padding-left: 3.5%;
	padding-right: 3.5%;
}

.routingMgtForm .routingAddBtn {
	display:none;
	padding: 0.4em 3em;	
}

.routingMgtForm .processInfo {
	width: 100%;
	text-align: center;
}

.routingMgtForm .grid-content {
	width:100%;
}

.routingMgtForm .grid-content .k-grid-toolbar{
	padding: 8px 8px; /** 200417 3단계 팝업 크기 맞추기용_LYM **/
}

