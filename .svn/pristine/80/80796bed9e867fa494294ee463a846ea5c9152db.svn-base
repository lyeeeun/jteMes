<!DOCTYPE html>
<html>
<body>
	<script type="text/javascript">
		var count=5;
		var counter = setInterval(timer, 1000);
		function timer(){
			count--;
			if(count<0){
				clearInterval(counter);
				goBack();
			}else{
				document.getElementById("goMsg").innerHTML = "Go back after "+count+" seconds...";
			}
		}
		function goBack(){
			if(document.referrer.lastIndexOf("login")==document.referrer.length-"login".length){
				location.href = document.referrer;
			}else{
				history.back(-1);
			}
		}
	</script>
	<h1>Something wrong!</h1>
	<p>Error Infomation</p>
	<table>
		<tr>
			<td>Error code</td><td style="color:#92B3B7" th:text="${code}">Error Code...</td>
		</tr>
		<tr>
			<td>Error Message</td><td style="color:#92B3B7" th:text="${message}">Error Message...</td>
		</tr>
	</table>
	<hr/>
	<a href="/">Go Home</a>
	<hr/>
	<a href="#" onclick="goBack();">Go Back</a>
	<hr/>
	<p id="goMsg">Go back after 5 seconds...</p>
</body>
</html>