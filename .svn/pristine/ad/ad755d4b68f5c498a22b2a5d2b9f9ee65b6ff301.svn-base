
@CHARSET "UTF-8";

.grid-content {
	width: 100%;
}

.rlehoMgtForm {
	width: 100%;
	margin: 0 auto;
/* 	text-align: center; */
	vertical-align: middle;
}

.rlehoMgtForm .left-contents {
	width: 20%;
	display: inline-block;
	height: 100%;
}

.rlehoMgtForm .middle-contents {
	width: 5%;
	display: inline-block;
	text-align: center;
	vertical-align: middle;
}

.rlehoMgtForm .right-contents {
	width: 74%;
	display: inline-block;
}

.middle-contents .fas.fa-caret-square-right.fa-3x{
	color: #3367D6;
	padding-right: 24px;
	
}

.rlehoMgtForm #jteMasterTree {
	font-size: 16px;
	border: 2px solid #8aafff;
	background:#ffffff;
}

/** 20.08.10 JJW 왼쪽 트리 구조 여백 따로 주기 위함**/
