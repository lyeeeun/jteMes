   width: 32%;
}

.toolMgtForm .col-right-title {
   width: 18%;
}

.toolMgtForm .col-right-content {
   width: 32%;
}

.toolMgtForm .jte-msgBox {
	width: 90%;
	float: right;
}

/*20.08.05 왼쪽 셀 가운데 정렬 JJW*/
