	width: 70%;
}

.toolMgtForm .group-checkbox {
	overflow: auto;
	height: 150px;
	background-color: #ffffff;
	padding-top: 5px;
	border: 1px solid #C7D5F4;
}

.toolMgtForm .center-input .btn-input {
	width: 100%;
}

.toolMgtForm .jtePopForm_master th {
   text-align: left;   
}

.toolMgtForm .left-input {
