<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://www.ehcache.org/v3"
    xmlns:jsr107="http://www.ehcache.org/v3/jsr107"
    xsi:schemaLocation="http://ehcache.org/ehcache.xsd
            http://www.ehcache.org/v3 
            http://www.ehcache.org/v3/jsr107
            http://www.ehcache.org/schema
            http://www.ehcache.org/schema/ehcache-core-3.0.xsd 
            http://www.ehcache.org/schema/ehcache-107-ext-3.0.xsd">
	
	<cache alias="cache_names"> 
		<key-type>java.lang.String</key-type> 
		<value-type>jin.mes.common.cache.CacheDto</value-type> 
		<expiry> 
			<ttl unit="minutes">60</ttl> 
		</expiry> 
		<resources> 
			<heap unit="entries">3</heap>
			<offheap unit="MB">100</offheap>
		</resources> 
	</cache> 
</config>